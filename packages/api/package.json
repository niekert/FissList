{
  "name": "api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "engines": {
    "node": "11.x.x"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.1.2",
    "apollo-server": "^2.2.2",
    "apollo-server-express": "^2.2.2",
    "body-parser": "^1.18.3",
    "camelcase-keys": "^5.0.0",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "express-history-api-fallback": "^2.2.1",
    "graphql": "^0.13",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "1.16.1",
    "lodash": "^4.17.11",
    "lodash.chunk": "^4.2.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.18.7",
    "prisma-client-lib": "^1.23.0-test.3",
    "query-string": "^6.2.0",
    "serve-static": "^1.13.2",
    "subscriptions-transport-ws": "^0.9.15",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/index.js",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000",
    "types": "apollo codegen:generate --queries=blaa --schema=./schema.graphql --target=typescript",
    "dev": "ts-node-dev src/index.ts",
    "watch-node": "dotenv -- nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/uuid": "^3.4.4",
    "babel-plugin-graphql-tag": "^1.6.0",
    "dotenv": "^6.1.0",
    "graphql-binding": "^2.2.6",
    "graphql-import": "^0.7.1",
    "prettier": "1.15.1",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "typescript": "^3.1.6"
  }
}

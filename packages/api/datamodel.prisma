type User {
  id: ID! @unique
  name: String!
}

type QueuedTrack {
  id: ID! @unique
  trackId: String!
  userVotes: [String!]!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type Party {
  id: ID! @unique
  name: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  ownerUserId: String!
  activeTrackId: String!
  queuedTracks: [QueuedTrack!]!
  requestedUserIds: [String!]!
  bannedUserIds: [String!]!
  partyUserIds: [String!]!
}

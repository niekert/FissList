type User {
  id: ID! @unique
  name: String!
}

type QueuedTrack {
  trackId: String!
  userVotes: [String!]!
}

type Party {
  id: ID! @unique
  name: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  ownerUserId: String!
  previousTrackUris: [String!]
  queuedTracks: [QueuedTrack!]!
  trackUris: [String!]!
  requestedUserIds: [String!]!
  bannedUserIds: [String!]!
  partyUserIds: [String!]!
}

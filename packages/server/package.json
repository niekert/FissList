{
  "name": "fisslist-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "camelcase-keys": "^5.0.0",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "graphql": "^0.13.2",
    "graphql-tools": "^4.0.2",
    "graphql-yoga": "^1.16.7",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.4",
    "prisma-client-lib": "^1.18.1"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/index.js",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000",
    "types": "apollo codegen:generate --queries=blaa --schema=./schema.graphql --target=typescript",
    "watch-node": "dotenv -- nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "devDependencies": {
    "dotenv": "^6.1.0",
    "graphql-binding": "^2.2.6",
    "prettier": "1.14.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  }
}
